<h2>Detailed Information for <%= @parent.name %></h2>
<h3><a href="/parents/<%= @parent.id %>/edit">Click to edit this record</a></h3>




<p>This station is <%= @parent.name %></p>

<% unless @parent.image.nil?  %>
  <img src="<%= @parent.image %>" alt="" class="feature">
<% end %>



  <iframe width="600" height="450" frameborder="0" style="border:0" src="<%= @gmapURL %>" allowfullscreen></iframe>



<p>Closest good place to eat is <%= @parent.fav_rest %></p>

<% unless @parent.fav_rest_img.nil?  %>
  <img src="<%= @parent.fav_rest_img %>" alt="" class="feature">
<% end %>


<ul>
  <li><%= @parent.name %></li>
  <li><%= @parent.mapref %></li>
  <li><%= @parent.image %></li>
  <li><%= @parent.fav_rest %></li>
  <li><%= @parent.fav_rest_img %></li>
</ul>





<h3>Connections to <%= @parent.name %></h3>
<!-- Gets list of all connections -->
 <% @children.each do |child| %>
   <% if child.parent_id == @parent.id %>
     <li>
      <strong><%= child.name %></strong>
     </li>
   <% end %>
 <% end %>

